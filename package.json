    "clean": "pnpm -r --filter='./packages/*' run clean && rimraf dist && rimraf node_modules"
  },
  "devDependencies": {
    "esbuild": "^0.16.12",
    "pnpm": "^7.21.0",
    "rimraf": "^3.0.2"
  }
}