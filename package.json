{
  "name": "stock-service",
  "version": "1.0.0",
  "main": "index.js",
  "author": "reftch",
  "license": "MIT",
  "scripts": {
    "install": "pnpm -r --filter='./packages/*' install",
    "dev": "pnpm -r --parallel --filter='./packages/*' run dev",
